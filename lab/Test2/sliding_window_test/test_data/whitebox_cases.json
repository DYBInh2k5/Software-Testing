[
  {
    "id": "WB01",
    "description": "Normal case - multiple windows",
    "arrays": [[1,2,3,4,5]],
    "n": 3,
    "expected_total": 3,
    "expected_windows": {"(1, 2, 3)": 1, "(2, 3, 4)": 1, "(3, 4, 5)": 1}
  },
  {
    "id": "WB02",
    "description": "Array shorter than n → skip",
    "arrays": [[1,2]],
    "n": 3,
    "expected_total": 0,
    "expected_windows": {}
  },
  {
    "id": "WB03",
    "description": "Exact length = n → 1 window",
    "arrays": [[7,7,7]],
    "n": 3,
    "expected_total": 1,
    "expected_windows": {"(7, 7, 7)": 1}
  },
  {
    "id": "WB04",
    "description": "Empty arrays list",
    "arrays": [],
    "n": 5,
    "expected_total": 0,
    "expected_windows": {}
  },
  {
    "id": "WB05",
    "description": "Duplicate windows across arrays",
    "arrays": [[1,1,1,1], [1,1,1,1]],
    "n": 2,
    "expected_total": 6,
    "expected_windows": {"(1, 1)": 6}
  },
  {
    "id": "WB06",
    "description": "n = 1 (boundary min)",
    "arrays": [[10,20,30]],
    "n": 1,
    "expected_total": 3,
    "expected_windows": {"(10,)": 1, "(20,)": 1, "(30,)": 1}
  },
  {
    "id": "WB07",
    "description": "Multiple different windows",
    "arrays": [[1,2,3,4,5,6]],
    "n": 4,
    "expected_total": 3,
    "expected_windows": {"(1, 2, 3, 4)": 1, "(2, 3, 4, 5)": 1, "(3, 4, 5, 6)": 1}
  }
]